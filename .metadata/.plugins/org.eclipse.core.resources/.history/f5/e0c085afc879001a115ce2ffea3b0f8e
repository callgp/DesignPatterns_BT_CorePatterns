package com.gp.patterns.singleton;

import java.io.File;
import java.io.FileNotFoundException;
import java.io.FileOutputStream;
import java.io.IOException;
import java.io.ObjectInput;
import java.io.ObjectInputStream;
import java.io.ObjectOutputStream;

public class Test {

	public static void main(String[] args) throws FileNotFoundException, IOException {

		DateUtil dateUtil1=DateUtil.getInstance();
		ObjectOutputStream oos=new ObjectOutputStream(new FileOutputStream(new File("C:\\gpFolder\\gpJava\\dateutil.ser")));
		oos.writeObject(dateUtil1);
		
		DateUtil dateUtil2=DateUtil.getInstance();
		ObjectOutputStream ois=new ObjectInputStream(new FileInputStream(new File("C:\\gpFolder\\gpJava\\dateutil.ser"));
		dateUtil=ois.readObject();
		
		System.out.println(dateUtil1==dateUtil2);
	}

}
